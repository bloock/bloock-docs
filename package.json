{
  "name": "bloock-docs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "dev": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier -w src docs",
    "format:docs": "prettier -w docs",
    "typecheck": "tsc",
    "spell-check": "npx cspell \"docs/**/*.{md,mdx}\""
  },
  "dependencies": {
    "@docusaurus/core": "^3.2.0",
    "@docusaurus/module-type-aliases": "^3.2.0",
    "@docusaurus/preset-classic": "^3.2.0",
    "@docusaurus/remark-plugin-npm2yarn": "^3.2.0",
    "@docusaurus/theme-live-codeblock": "^3.2.0",
    "@graphglue/dokka-docusaurus": "^1.0.2",
    "@stoplight/elements": "^8.1.0",
    "@styled-icons/boxicons-logos": "^10.47.0",
    "@styled-icons/fluentui-system-regular": "^10.47.0",
    "clsx": "^2.1.0",
    "param-case": "^4.0.0",
    "prism-react-renderer": "^2.3.1",
    "raw-loader": "^4.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10",
    "styled-components": "^5.3.11"
  },
  "devDependencies": {
    "@swc/core": "^1.4.11",
    "@tsconfig/docusaurus": "^2.0.3",
    "@types/react": "^18.2.74",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "autoprefixer": "^10.4.19",
    "buffer": "^6.0.3",
    "docusaurus-plugin-typedoc": "^0.22.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "fs-extra": "^11.2.0",
    "js-yaml": "^4.1.0",
    "jsdom": "^24.0.0",
    "pascal-case": "^4.0.0",
    "path-browserify": "^1.0.1",
    "postcss": "^8.4.38",
    "postcss-import": "^16.1.0",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "process": "^0.11.10",
    "react-markdown": "^9.0.1",
    "stream-browserify": "^3.0.0",
    "swc-loader": "^0.2.6",
    "tailwindcss": "^3.4.3",
    "typedoc": "^0.25.12",
    "typedoc-plugin-markdown": "^3.17.1",
    "typescript": "^5.4.3",
    "url": "^0.11.3"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.14"
  },
  "prettier": {
    "singleQuote": true,
    "tabWidth": 2
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
